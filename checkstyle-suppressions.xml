<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
  "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>
<!--  Suppress AbbreviationAsWordInName by default because the ODE contains many abbreviations of data types
      and using those abbreviations is more helpful than spelling out the words-->
  <suppress checks="AbbreviationAsWordInName"
    files=".*\.java"/>
</suppressions>